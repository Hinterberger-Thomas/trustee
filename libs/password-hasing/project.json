{
  "root": "libs/password-hasing",
  "sourceRoot": "libs/password-hasing/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/password-hasing/**/*.ts"]
      }
